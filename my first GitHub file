// Example program
#include <iostream>
#include <string>

int check_number(int a)
{
  int num=a;
  switch(num)
  {
    case 1: if(num==5){
         return num;
        }
    case 2: if(num==15){
        return num;
        }
    case 3: if(num==25){
        return num;
        }
    case 4: if(num==11){
        return num;
        }
    case 4: if(num==10){
        return num;
        }
  }
  
}

int main()
{
  std::string name;
  std::cout << "What is your name? ";
  getline (std::cin, name);
  std::cout << "Hello, " << name << "!\n";
}
